{
  "name": "@ohos/hvigor-arkts-compose",
  "version": "5.8.5",
  "description": "hvigor-arkts-compose",
  "main": "./dist/index.js",
  "scripts": {
    "patch-publish": "npm --no-git-tag-version version prepatch&&npm publish",
    "prepack": "npm run ugly-build",
    "build": "tsc -p . && npm run copy-native",
    "watch": "tsc --watch",
    "clean": "tsc --build --clean",
    "rebuild": "npm run clean&&npm run build",
    "uglify": "node uglify.mjs",
    "copy-native": "node copy-native.js",
    "ugly-build": "npm run clean && npm run build && npm run uglify",
    "test": "jest",
    "coverage": "nyc --clean jest --coverage",
    "fix": "eslint ./**/*.ts --fix"
  },
  "dependencies": {
    "@ohos/hvigor-arkts-base": "5.8.5",
    "@ohos/coverage": "1.0.33",
    "@rollup/plugin-commonjs": "24.1.0",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-terser": "0.3.0",
    "@babel/core": "7.23.0",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-json": "6.0.0",
    "@babel/plugin-transform-class-properties": "7.22.5",
    "magic-string": "0.27.0",
    "glob": "10.3.12",
    "js-beautify": "1.14.11",
    "@rollup/pluginutils": "5.0.2",
    "@baize/wdk": "0.4.0"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.node",
    "dist/**/*.d.ts",
    "res/**/*"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "author": "huawei",
  "ohos": {
    "org": "huawei"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "keywords": [
    "hvigor",
    "OpenHarmony"
  ]
}
