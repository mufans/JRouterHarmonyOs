    "report": "node ./lib/src/generateReport.js"
  },
  "dependencies": {
    "fs-extra": "11.2.0",
    "istanbul-reports": "3.1.6",
    "log4js": "6.7.1",
    "source-map": "0.7.4"
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "res/**/*"
  ],
  "keyword": [
    "coverage"
  ],
  "devDependencies": {
    "magic-string": "0.30.3",
    "@types/jest": "29.5.3",
    "@types/node": "20.11.19",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "eslint": "8.46.0",
    "jest": "29.6.2",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  },
  "ohos": {
    "org": "huawei"
  },
